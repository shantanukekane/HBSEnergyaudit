<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5">

<listeners>
<listener class-name="utilities.ExtendReportManager"></listener>
</listeners>

<test name="Window-Chrome">

<parameter name="os" value="Window"/>
<parameter name="browser" value="Chrome"/>

<classes>
      <class name="testCases.TC_001_AccountRegistrationTest"/>
      <class name="testCases.TC_002_LoginTest"/>
</classes>

</test>

<test name="Window-Edge">

<parameter name="os" value="Window"/>
<parameter name="browser" value="Edge"/>

<classes>
      <class name = "testCases.TC_004_SearchProductTest"/>
      <class name="testCases.TC_005_AddToCartProductTest"/>
</classes>

</test>
</suite>